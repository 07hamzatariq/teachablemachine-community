{
  "name": "@teachablemachine/image",
  "version": "0.1.0",
  "description": "A support library for use with mobilenet-based models generated by Teachable Machine (https://g.co/teachablemachine)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build-bundle",
    "build-bundle": "webpack --reporter=basic --mode=production",
    "build-clean": "rm -rf ./dist && rm -rf ./bundles && npm run build",
    "dev": "webpack --watch --reporter=basic --mode=development",
    "lint": "tslint -p . -t verbose",
    "make-version": "node scripts/make-version.js",
    "upload": "gsutil -m cp -r ./bundles/** gs://tm-pro",
    "upload-tarball": "npm pack && gsutil -m cp ./*.tgz gs://tm-pro-tgz",
    "update-release": "npm run build && npm run upload",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc"
  },
  "license": "Apache-2",
  "peerDependencies": {
    "@tensorflow/tfjs": "1.1.2"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^1.1.2",
    "lodash.clonedeep": "^4.5.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^5.3.1",
    "tslint": "^5.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  }
}
