{
  "name": "@teachablemachine/pose",
  "version": "0.4.0",
  "description": "A support library for use with posenet-based models generated by Teachable Machine (https://g.co/teachablemachine)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build-bundle",
    "build-bundle": "webpack --reporter=basic --mode=production",
    "build-clean": "rm -rf ./dist && rm -rf ./bundles && npm run build",
    "dev": "concurrently \"npm run dev-lite\" \"npm run test-watch\"",
    "dev-lite": "webpack --watch --reporter=basic --mode=development",
    "lint": "tslint -p . -t verbose",
    "make-version": "node scripts/make-version.js",
    "publish-local": "npm run build-clean && yalc publish",
    "update-release": "npm run build && npm run upload",
    "upload": "gsutil -m cp -r ./bundles/** gs://tm-pro",
    "upload-tarball": "npm pack && gsutil -m cp ./*.tgz gs://tm-pro-tgz",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "tsc": "tsc"
  },
  "license": "Apache-2",
  "dependencies": {
    "@tensorflow-models/posenet": "^2.1.0"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "1.1.2"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "1.1.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^6.1.4",
    "puppeteer": "^1.18.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^5.3.1",
    "tslint": "^5.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "yalc": "^1.0.0-pre.32"
  }
}
